{
  "name": "Vuebly",
  "description": "A Weex UI framework for WeChat, Native iOS and Android App",
  "version": "1.0.0",
  "author": "Louie Bao <louie.bao@vuebly.com>",
  "private": true,
  "keywords": [
    "weex",
    "vue",
    "vuebly",
    "wechat",
    "android",
    "ios"
  ],
  "scripts": {
    "build": "rm -rf dist && rm -rf src/entry && node build/webpack.build.conf.js",
    "copy:ios": "rm -rf ios/assets && cp -rf dist/weex ios/assets",
    "copy:android": "rm -rf android/app/src/main/assets && cp -rf dist/weex android/app/src/main/assets",
    "copy": "npm run copy:ios && npm run copy:android",
    "debug": "weex-devtool",
    "dev": "rm -rf src/entries && node build/webpack.dev.conf.js",
    "lint": "eslint --ext .js,.vue src"
  },
  "dependencies": {
    "vue": "^2.3.3",
    "vue-router": "^2.6.0",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "ip": "^1.1.5",
    "loglevel": "^1.4.1",
    "node-sass": "^4.5.3",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "ora": "^1.3.0",
    "postcss-cssnext": "^3.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "serve": "^6.0.2",
    "url-loader": "^0.5.8",
    "vue-loader": "^10.3.0",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0",
    "weex-builder": "^0.2.6",
    "weex-loader": "^0.4.5",
    "weex-vue-render": "^0.11.57"
  }
}
